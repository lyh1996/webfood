<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <listener>
    <listener-class>com.yc.web.listerner.InitListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.yc.web.listerner.CleanRedisOnlineUserListener</listener-class>
  </listener>
  <display-name>Goods</display-name>
  <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.yc.web.filters.CharacterEncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
   <servlet>
    <servlet-name>HistroyServlet</servlet-name>
    <servlet-class>com.yc.web.servlet.HistroyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HistroyServlet</servlet-name>
    <url-pattern>/Scan.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.yc.web.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>resfoodServlet</servlet-name>
    <servlet-class>com.yc.web.servlet.ResfoodServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>resfoodServlet</servlet-name>
    <url-pattern>/resfood.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>verifyCodeServlet</servlet-name>
    <servlet-class>com.yc.web.servlet.VerifyCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>verifyCodeServlet</servlet-name>
    <url-pattern>/verifyCode.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>resuserServlet</servlet-name>
    <servlet-class>com.yc.web.servlet.ResuserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>resuserServlet</servlet-name>
    <url-pattern>/resuser.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ResorderServlet</servlet-name>
    <servlet-class>com.yc.web.servlet.ResorderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResorderServlet</servlet-name>
    <url-pattern>/resorder.action</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/404.html</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/500.html</location>
  </error-page>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>